{
  "expo": {
    "name": "Shopping Center Building Management App",
    "slug": "Shopping Center Building Management App",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/Building-Logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/building-logo-transparent.png",
        "backgroundColor": "#000"
      },
      "package": "com.btlnt.shoppingcenter"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Building-Logo.png",
        "resizeMode": "contain"
        "backgroundColor": "#fff"
      },
      "package": "com.btlnt.shoppingcenter"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Building-Logo.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d4deff52-4dce-4307-b8a3-311872a2c548"
      }
    }
  }
}

